first method:
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x,a,b;
    cin>>x;
    a=x%10;
    b=x/10;
    if(a>=5)
    {
        cout<<(b+1)*10;
    }
    else
    {
        cout<<b*10;
    }
}
second method:
#include<bits/stdc++.h>
using namespace std;
int iwantanumber(int d)
{
    int a,b;
    a=(d/10)*10;   
    b=a+10;      
    return(d-a>=b-d)?b:a; 
}
 int main()
 {
     int d;
     cin>>d;
     cout<<iwantanumber(d);
     return 0;
 }
