#include<iostream>
using namespace std;
int main()
{
    int a,i;
    cin>>a;
    int b[a],c[a];
    for(i=0;i<a;i++)
    {
        cin>>b[i];
    }
    for(i=0;i<a;i++)
    {
        cin>>c[i];
    }
    for(i=0;i<a;i++)
    {
        for(i=0;i<a;i++)
      {
         cout<<b[i]+c[i]<<" ";
      }
    }
}
