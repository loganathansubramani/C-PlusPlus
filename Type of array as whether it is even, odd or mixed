#include<iostream>
using namespace std;
int main()
{
    int a,i,odd=0,even=0;
    cin>>a;
    int b[a];
    for(i=0;i<a;i++)
    {
        cin>>b[i];
    }
    for(i=0;i<a;i++)
    {
        if(b[i]%2!=0)
        {
            odd++;
        }
        else
        {
            even++;
        }
    }
    if(odd==a)
    {
        cout<<"Odd";
    }
    else if(even==a)
    {
         cout<<"Even";
    }
    else 
    {
         cout<<"Mixed";
    }

    
}
