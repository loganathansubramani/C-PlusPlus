#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    int a,b,c,d,e;
    cin>>a;
    b=(a/100);
    c=(a%100)/10;
    d=(a%100)%10;
    e=((b^3)+(c^3)+(d^3));
    if(a==e)
    {
        cout<<"Armstrong Number";
    }
    else 
    {
        cout<<"Not an Armstrong Number";
    }
}
