#include<iostream>
using namespace std;
int main()
{
    int a,i,c=0;
    cin>>a;
    for(i=0;i<3;i++)
    {
        for(i=1;i<100;i++)
        {
            if(c<=3)
            {
                if(a%i==0)
                {
                    cout<<i<<" ";
                    c++;
                }
            }
        }
    }
}
