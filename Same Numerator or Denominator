#include <iostream> 
using namespace std; 
int main() 
{ int numerator1, denominator1, numerator2, denominator2; 
char ch1, ch2; 
cin >> numerator1 >> ch1 >> denominator1 >> numerator2 >> ch2 >> denominator2; 
if(numerator1 == numerator2 || denominator1 == denominator2)
{ 
    cout << "yes"; 
    
} 
else 
{ 
    cout << "no"; 
    
} 
return 0;
}
