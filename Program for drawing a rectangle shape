#include <iostream>
using namespace std;
int main() 
{
   int length,breadth;
   char symbol;
   cout<<"Enter length:";
   cin>>length;
   cout<<"Enter breadth:";
   cin>>breadth;
   cout<<"Enter symbol:";
   cin>>symbol;
   for(int i=0;i<length;i++)
   {
       for(int j=0;j<breadth;j++)
       {
           cout<<symbol<<" ";
       }
       cout<<endl;
   }
}
